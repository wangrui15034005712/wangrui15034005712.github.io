<div class="widget-wrap" style='width:225px;margin-top:50px;'>
  <h3 class="widget-title"></h3>
  <div class="widget">
    <h1 class="blog-title"><%= config.title %></h1>
    <h2 class="blog-subtitle"><%= config.subtitle %></h2>
    <ul class="blog-link">
     <% for (let item in theme.menu){ %>
          <a href="<%= theme.menu[item] %>" title="<%= item %>">
            <li><%= __(item) %></li>
          </a>
        <% } %>
    </ul>
  </div>
</div>
